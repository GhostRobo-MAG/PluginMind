  "scripts": {
    "dev": "contentlayer build && next dev",
    "build": "contentlayer build && next build",
    "start": "next start",
    "shadcn:add": "pnpm dlx shadcn-ui@latest add",
    "packages:update-all": "pnpm update --latest",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit",
    "lint": "contentlayer build && (next lint || echo '⚠️  Linting completed with warnings - build will continue')",
    "lint:fix": "contentlayer build && next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm test:ci && pnpm test:e2e"
  },
